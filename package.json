{
  "name": "psarcjs",
  "version": "0.1.4",
  "description": "node module to read and extract Rocksmith PSARC files",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "drypack": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "cover": "c8 --reporter=html --reporter=text mocha ./test/test.js",
    "coverage": "c8 report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandiz/psarcjs.git"
  },
  "files": [
    "index.js",
    "parser.js"
  ],
  "keywords": [
    "psarc",
    "psarcjs",
    "rocksmith",
    "rocksmith2014"
  ],
  "author": "Sandi Chakravarty <sandi.newton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sandiz/psarcjs/issues"
  },
  "homepage": "https://github.com/sandiz/psarcjs#readme",
  "dependencies": {
    "aes-js": "^3.1.2",
    "binary-parser": "^1.3.2"
  },
  "devDependencies": {
    "c8": "^5.0.1",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "coveralls": "^3.0.3",
    "mocha": "^6.1.4",
    "tmp": "^0.1.0"
  }
}